[user]
	email = a347liao@gmail.com
	name = duckll
[color]
    ui = true
[alias]
	a = add
	b = branch
	c = commit
	cl = clone
	cm = commit -m
	co = checkout
	cp = cherry-pick
	d = diff --color=auto
	dc = diff --color=auto --cached
	diff = diff --color=auto
	g = grep --break --heading --line-number --color=auto
	l = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%ai%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	p = pull
	s = status
[core]
    editor = vim
	excludesfile = /Users/duckll/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Users/duckll/Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
